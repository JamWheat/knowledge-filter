<%- include('../partials/header') %> 

<h1><%= title %></h1>

<form action="/questions" method="POST">
  Description:<br>
  <input type="text" name="subject" maxlength="42" required size="26" autocomplete="off" placeholder="be brief, but specific"><br>
  <br>
  Question:<br>
  <textarea rows="3" name="content" required cols="26" autocomplete="off" placeholder="tell us what Knowledge you lack"></textarea><br>
  <br>
  <input type="submit" class="button">

</form>


      
<%- include('../partials/footer') %> 