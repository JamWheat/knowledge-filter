<%- include('../partials/header') %> 

<h1><%= title %></h1>
<div class="list">
<% questions.forEach((q) =>{ %>
  <% if(q.isPublic) { %> 
    <div>
      <a href="/questions/<%= q._id %> ">
        <h3><%= q.subject %> </h3>
      </a>
      <span class="righty"><h3><i class="fas fa-thumbs-up"></i>: <%=q.likedBy.length%></h3></span>
    </div>
  <% } %>
<% }) %> 
</div>
      
<%- include('../partials/footer') %> 